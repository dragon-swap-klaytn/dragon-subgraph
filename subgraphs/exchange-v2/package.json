{
  "name": "exchange-v2",
  "description": "PancakeSwap Subgraph",
  "version": "1.0.7",
  "repository": "git@github.com:dragon-swap-klaytn/dragon-subgraph.git",
  "author": "PancakeSwap",
  "license": "GPL-3.0-or-later",
  "config": {
    "testnet": {
      "graph_node": "http://0.0.0.0:8020/",
      "ipfs_url": "http://0.0.0.0:5001"
    },
    "cypress": {
      "graph_node": "http://0.0.0.0:8020/",
      "ipfs_url": "http://0.0.0.0:5001"
    }
  },
  "scripts": {
    "template": "mustache ../../config/$NETWORK.js subgraph.template.yaml > subgraph.yaml && mustache ../../config/$NETWORK.js mappings/utils/index.template.ts > mappings/utils/index.ts && mustache ../../config/$NETWORK.js mappings/pricing.template.ts > mappings/pricing.ts",
    "auth": "graph auth --product hosted-service SUBGRAPH_ACCESS_TOKEN",
    "codegen": "graph codegen subgraph.yaml",
    "build": "graph build subgraph.yaml",
    "create:cypress": "graph create  --network cypress --node $npm_package_config_cypress_graph_node dragonswap/exchange-v2",
    "deploy:cypress": "graph deploy  --network cypress --node $npm_package_config_cypress_graph_node --ipfs $npm_package_config_cypress_ipfs_url -l 0.3 dragonswap/exchange-v2",
    "all:cypress": "NETWORK=cypress yarn template && yarn codegen && yarn build && yarn create:cypress && yarn deploy:cypress",
    "create:local": "graph create --node http://host.docker.internal:8020/ dgswap-exchange-v2-kaia",
    "deploy:local": "graph deploy --node http://host.docker.internal:8020/ --ipfs http://host.docker.internal:5001 --version-label $npm_package_version dgswap-exchange-v2-kaia",
    "create:kaiagraph": "graph create --node https://admin-graph.kaia.io/mainnet/graph/admin dgswap-exchange-v2-kaia",
    "deploy:kaiagraph": "graph deploy --node https://admin-graph.kaia.io/mainnet/graph/admin --ipfs https://admin-graph.kaia.io/ipfs/api/v0 --version-label $npm_package_version dgswap-exchange-v2-kaia",
    "deploy:kaia": "graph deploy --studio --version-label $npm_package_version dgswap-exchange-v2-kaia",
    "all:kaia": "NETWORK=kaia npm run template && npm run codegen && npm run build && npm run deploy:kaia"
  }
}
